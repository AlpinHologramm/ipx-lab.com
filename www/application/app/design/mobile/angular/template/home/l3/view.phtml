<div id="tabbar_items" class="overview-tabbar tabbar_items type_scroll slide-top {{ features.layoutId }}" ng-show="tabbar_is_visible">
    <ul id="tabbar" class="tabbar_l3">
        <li>
            <a ng-repeat="option in features.overview.options" href="{{ option.url }}" class="item" ng-class="{ is_link: option.is_link }">
                <div class="relative">
                    <div class="tabbar">
                        <img ng-src="{{ option.icon_url }}" ng-class="{ colorable: option.icon_is_colorable }" width="55" height="55" />
                    </div>
                    <span class="tabbar no-background block">{{ option.name }}</span>
                </div>
            </a>
        </li>
    </ul>
</div>